{
    "name": "innoboxrr/twilio-sdk",
    "version": "1.0.0",
    "description": "Twilio package",
    "keywords": [
        "innoboxrr",
        "twilio-sdk"
    ],
    "homepage": "https://github.com/innoboxrr/twilio-sdk",
    "license": "The Unlicense",
    "type": "library",
    "authors": [
        {
            "name": "Homero Raul Vargas Cruz",
            "email": "dev@innobox.systmes",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "twilio/sdk": "^7.4"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Innoboxrr\\TwilioSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Innoboxrr\\TwilioSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "extra": {
        "laravel": {
            "providers": [
                "Innoboxrr\\TwilioSdk\\Providers\\TwilioSdkServiceProvider"
            ]
        }
    }
}
